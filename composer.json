{
    "name": "mola/larepository",
    "description": "An artisan command which creates repositories for eloquent-models.",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "mockery/mockery": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "orchestra/testbench": "3.5.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Mola\\Larepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mola\\Larepository\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mola\\Larepository\\LarepositoryServiceProvider"
            ]
        }
    }
}
